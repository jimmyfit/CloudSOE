{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "resources": [
        {
            "type": "Microsoft.ManagedIdentity/userAssignedIdentities",
            "name": "AIB",
            "apiVersion": "2018-11-30",
            "location": "[resourceGroup().location]"
        },
      {
        "name": "WindowsServer2019SOE",
        "type": "Microsoft.VirtualMachineImages/imageTemplates",
        "apiVersion": "2020-02-14",
        "location": "eastus",
        "dependsOn": [
            "[resourceId('Microsoft.ManagedIdentity/userAssignedIdentities', 'AIB')]"
        ],
        "identity": {
            "type": "UserAssigned",
                    "userAssignedIdentities": {
                    "[resourceId('Microsoft.ManagedIdentity/userAssignedIdentities', 'AIB')]": {}
                        
                }
        },
        "properties": {
            "buildTimeoutInMinutes" : 100,

            "vmProfile": 
                    {
                    "vmSize": "Standard_D1_v2",
                    "osDiskSizeGB": 127
                    },
        
            "source": {
                "type": "PlatformImage",
                    "publisher": "MicrosoftWindowsServer",
                    "offer": "WindowsServer",
                    "sku": "2019-Datacenter",
                    "version": "latest"
                
            },
            "customize": [

            ],
            "distribute": 
            [
                {   
                    "type": "SharedImage",
                    "galleryImageId": "[concat('/subscriptions/',subscription().id,'/resourceGroups/',resourceGroup().location,'/providers/Microsoft.Compute/galleries/CloudSOESIG/images/WinServer2019-SOE')]",
                    "runOutputName": "huh",
                    "artifactTags": {
                        "source": "azureVmImageBuilder",
                        "baseosimg": "windows2019"
                    },
                    "replicationRegions": [
                      "australiacentral",
                      "australiacentral2"
                    ]
                }
            ]
            }
        }
    ]
}    